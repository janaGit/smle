<?xml version="1.0" encoding="UTF-8"?>
<p:profile xmlns:p="http://xmlProfile.de" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlProfile.de Profile.xsd" class="sml:PhysicalSystem">

	<p:elementString XPath="gml:description">
		<p:restrictions>
			<p:use>required</p:use>
		</p:restrictions>
		<p:input>
			<p:textField />
		</p:input>
	</p:elementString>

	<p:elementString XPath="sml:keywords/sml:KeywordList/sml:keyword">
		<p:restrictions>
			<p:use>optional</p:use>
		</p:restrictions>
		<p:input>
			<p:textField />
		</p:input>
	</p:elementString>

	<p:elementString XPath="sml:identification/sml:IdentifierList/sml:identifier/sml:Term/sml:label">
		<p:restrictions>
			<p:use>required</p:use>
		</p:restrictions>
		<p:input>
			<p:textField />
		</p:input>
	</p:elementString>

	<p:elementString XPath="sml:identification/sml:IdentifierList/sml:identifier/sml:Term/sml:value">
		<p:restrictions>
			<p:use>required</p:use>
		</p:restrictions>
		<p:input>
			<p:textField />
		</p:input>
	</p:elementString>

	<p:elementString XPath="sml:identification/sml:IdentifierList/sml:identifier/sml:Term/@definition">
		<p:restrictions>
			<p:use>required</p:use>
		</p:restrictions>
		<p:input>
			<p:textField />
		</p:input>
	</p:elementString>

	<p:elementString
		XPath="sml:contacts/sml:ContactList/sml:contact/gmd:CI_ResponsibleParty/gmd:organisationName">
		<p:restrictions>
			<p:use>required</p:use>
		</p:restrictions>
		<p:input>
			<p:textField />
		</p:input>
	</p:elementString>

	<p:elementString
		XPath="sml:position/swe:DataRecord/swe:field/swe:Vector/swe:coordinate/swe:Quantity/swe:uom/@code">
		<p:restrictions>
			<p:use>required</p:use>
		</p:restrictions>
		<p:input>
			<p:textField />
		</p:input>
	</p:elementString>
	<p:elementDouble
		XPath="sml:position/swe:DataRecord/swe:field/swe:Vector/swe:coordinate/swe:Quantity/swe:value">
		<p:restrictions>
			<p:use>required</p:use>
		</p:restrictions>
		<p:input>
			<p:numberField />
		</p:input>
	</p:elementDouble>
	<p:formConfiguration>
		<p:formComponent label="Form">
			<p:complexElementInstance groupRef="Identification" />
			<p:complexElementInstance groupRef="TimePeriod" />
			<p:complexElementInstance groupRef="Position" />
		</p:formComponent>
	</p:formConfiguration>
	<p:complexElementInstance groupID="Identification" XPath="sml:identification/sml:IdentifierList">
		<p:elements>
			<p:complexElementInstanceRef groupRef="uniqueID"
				XPath="sml:identification/sml:IdentifierList/sml:identifier/sml:Term" />
			<p:complexElementInstanceRef groupRef="shortName"
				XPath="sml:identification/sml:IdentifierList/sml:identifier/sml:Term" />
			<p:complexElementInstanceRef groupRef="longName"
				XPath="sml:identification/sml:IdentifierList/sml:identifier/sml:Term" />
			<p:complexElementInstanceRef groupRef="deviceID"
				XPath="sml:identification/sml:IdentifierList/sml:identifier/sml:Term" />
			<p:complexElementInstanceRef groupRef="serialNumber"
				XPath="sml:identification/sml:IdentifierList/sml:identifier/sml:Term" />
			<p:complexElementInstanceRef groupRef="WMO"
				XPath="sml:identification/sml:IdentifierList/sml:identifier/sml:Term" />
		</p:elements>
	</p:complexElementInstance>

	<p:complexElementInstance groupID="shortName"
		XPath="sml:identification/sml:IdentifierList/sml:identifier/sml:Term">
		<p:elements>
			<p:elementInstanceString XPath="sml:label" ID="label_shortName">
				<p:restrictions>
					<p:fixContent>
						<p:value>Short name</p:value>
					</p:fixContent>
				</p:restrictions>
				<p:input>
					<p:textField hide="true" />
				</p:input>
			</p:elementInstanceString>
			<p:elementInstanceAnyURI XPath="@definition" ID="definition_shortName">
				<p:restrictions>
					<p:fixContent>
						<p:value>http://www.nexosproject.eu/dictionary/definitions.html#shortName</p:value>
					</p:fixContent>
				</p:restrictions>
				<p:input>
					<p:textField hide="true" />
				</p:input>
			</p:elementInstanceAnyURI>
			<p:elementInstanceString XPath="sml:value" ID="value_shortName">
				<p:label>Short name (sml:value)</p:label>
			</p:elementInstanceString>
		</p:elements>
	</p:complexElementInstance>

	<p:complexElementInstance groupID="longName"
		XPath="sml:identification/sml:IdentifierList/sml:identifier/sml:Term">
		<p:elements>
			<p:elementInstanceString XPath="sml:label" ID="label_longName">
				<p:restrictions>
					<p:fixContent>
						<p:value>Long name</p:value>
					</p:fixContent>
				</p:restrictions>
				<p:input>
					<p:textField hide="true" />
				</p:input>
			</p:elementInstanceString>
			<p:elementInstanceAnyURI XPath="@definition" ID="definition_longName">
				<p:restrictions>
					<p:fixContent>
						<p:value>http://www.nexosproject.eu/dictionary/definitions.html#longName</p:value>
					</p:fixContent>
				</p:restrictions>
				<p:input>
					<p:textField hide="true" />
				</p:input>
			</p:elementInstanceAnyURI>
			<p:elementInstanceString XPath="sml:value" ID="value_longName">
				<p:label>Long name (sml:value)</p:label>
			</p:elementInstanceString>
		</p:elements>
	</p:complexElementInstance>
	<p:complexElementInstance groupID="uniqueID"
		XPath="sml:identification/sml:IdentifierList/sml:identifier/sml:Term">
		<p:elements>
			<p:elementInstanceString XPath="sml:label" ID="label_uniqueID">
				<p:restrictions>
					<p:fixContent>
						<p:value>UUID</p:value>
					</p:fixContent>
				</p:restrictions>
				<p:input>
					<p:textField hide="true" />
				</p:input>
			</p:elementInstanceString>
			<p:elementInstanceAnyURI XPath="@definition" ID="definition_uniqueID">
				<p:restrictions>
					<p:fixContent>
						<p:value>http://www.nexosproject.eu/dictionary/definitions.html#UUID</p:value>
					</p:fixContent>
				</p:restrictions>
				<p:input>
					<p:textField hide="true" />
				</p:input>
			</p:elementInstanceAnyURI>
			<p:elementInstanceString XPath="sml:value" ID="value_uniqueID">
				<p:label>Unique ID (sml:value)</p:label>
			</p:elementInstanceString>
		</p:elements>
	</p:complexElementInstance>
	<p:complexElementInstance groupID="deviceID"
		XPath="sml:identification/sml:IdentifierList/sml:identifier/sml:Term">
		<p:elements>
			<p:elementInstanceString XPath="sml:label" ID="label_deviceID">
				<p:restrictions>
					<p:fixContent>
						<p:value>Device ID</p:value>
					</p:fixContent>
				</p:restrictions>
				<p:input>
					<p:textField hide="true" />
				</p:input>
			</p:elementInstanceString>
			<p:elementInstanceAnyURI XPath="@definition" ID="definition_deviceID">
				<p:restrictions>
					<p:fixContent>
						<p:value>http://www.nexosproject.eu/dictionary/definitions.html#deviceID</p:value>
					</p:fixContent>
				</p:restrictions>
				<p:input>
					<p:textField hide="true" />
				</p:input>
			</p:elementInstanceAnyURI>
			<p:elementInstanceString XPath="sml:value" ID="value_deviceID">
				<p:label>Device ID (sml:value)</p:label>
			</p:elementInstanceString>
		</p:elements>
	</p:complexElementInstance>
	<p:complexElementInstance groupID="serialNumber"
		XPath="sml:identification/sml:IdentifierList/sml:identifier/sml:Term">
		<p:elements>
			<p:elementInstanceString XPath="sml:label" ID="label_serialNumber">
				<p:restrictions>
					<p:fixContent>
						<p:value>Serial number</p:value>
					</p:fixContent>
				</p:restrictions>
				<p:input>
					<p:textField hide="true" />
				</p:input>
			</p:elementInstanceString>
			<p:elementInstanceAnyURI XPath="@definition" ID="definition_serialNumber">
				<p:restrictions>
					<p:fixContent>
						<p:value>http://www.nexosproject.eu/dictionary/definitions.html#serialNumber</p:value>
					</p:fixContent>
				</p:restrictions>
				<p:input>
					<p:textField hide="true" />
				</p:input>
			</p:elementInstanceAnyURI>
			<p:elementInstanceString XPath="sml:value" ID="value_serialNumber">
				<p:label>Serial number (sml:value)</p:label>
			</p:elementInstanceString>
		</p:elements>
	</p:complexElementInstance>
	<p:complexElementInstance groupID="WMO"
		XPath="sml:identification/sml:IdentifierList/sml:identifier/sml:Term">
		<p:elements>
			<p:elementInstanceString XPath="sml:label" ID="label_WMO">
				<p:restrictions>
					<p:fixContent>
						<p:value>WMO</p:value>
					</p:fixContent>
				</p:restrictions>
				<p:input>
					<p:textField hide="true" />
				</p:input>
			</p:elementInstanceString>
			<p:elementInstanceAnyURI XPath="@definition" ID="definition_WMO">
				<p:restrictions>
					<p:fixContent>
						<p:value>http://www.nexosproject.eu/dictionary/definitions.html#WMO_ID</p:value>
					</p:fixContent>
				</p:restrictions>
				<p:input>
					<p:textField hide="true" />
				</p:input>
			</p:elementInstanceAnyURI>
			<p:elementInstanceString XPath="sml:value" ID="value_WMO">
				<p:label>WMO (sml:value)</p:label>
			</p:elementInstanceString>
		</p:elements>
	</p:complexElementInstance>

	<p:complexElementInstance groupID="TimePeriod" XPath="sml:validTime/gml:TimePeriod">
		<p:elements>
			<p:elementInstanceString ID="ID_TimePeriod" XPath="gml:id">
				<p:restrictions>
					<p:fixContent>
						<p:value>validityPeriod</p:value>
					</p:fixContent>
				</p:restrictions>
			</p:elementInstanceString>
		</p:elements>
	</p:complexElementInstance>


	<p:complexElementInstance groupID="Position" XPath="sml:position/swe:DataRecord">
		<p:elements>
			<p:complexElementInstance groupID="location_field" XPath="swe:field">
				<p:elements>
					<p:elementInstanceNCName ID="name_location_field" XPath="@name">
						<p:restrictions>
							<p:fixContent>
								<p:value>location</p:value>
							</p:fixContent>
						</p:restrictions>
					</p:elementInstanceNCName>
					<p:complexElementInstance groupID="vector_location_field" XPath="swe:Vector">
						<p:elements>
							<p:complexElementInstance groupID="latitude_coordinate" XPath="swe:coordinate">
								<p:elements>
									<p:elementInstanceNCName ID="name_latitude_coordinate" XPath="@name">
										<p:restrictions>
											<p:fixContent>
												<p:value>Lat</p:value>
											</p:fixContent>
										</p:restrictions>
									</p:elementInstanceNCName>
									<p:complexElementInstanceRef groupRef="latitude_Quantity"
										XPath="swe:coordinate/swe:Quantity" />
								</p:elements>
							</p:complexElementInstance>
							<p:complexElementInstance groupID="longitude_coordinate" XPath="swe:coordinate">
								<p:elements>
									<p:elementInstanceNCName ID="name_longitude_coordinate" XPath="@name">
										<p:restrictions>
											<p:fixContent>
												<p:value>Lon</p:value>
											</p:fixContent>
										</p:restrictions>
									</p:elementInstanceNCName>
									<p:complexElementInstanceRef groupRef="longitude_Quantity"
										XPath="swe:coordinate/swe:Quantity" />
								</p:elements>
							</p:complexElementInstance>
							<p:complexElementInstance groupID="altitude_coordinate" XPath="swe:coordinate">
								<p:elements>
									<p:elementInstanceNCName ID="name_altitude_coordinate" XPath="@name">
										<p:restrictions>
											<p:fixContent>
												<p:value>Alt</p:value>
											</p:fixContent>
										</p:restrictions>
									</p:elementInstanceNCName>
									<p:complexElementInstanceRef groupRef="altitude_Quantity"
										XPath="swe:coordinate/swe:Quantity" />
								</p:elements>
							</p:complexElementInstance>
						</p:elements>
					</p:complexElementInstance>

				</p:elements>
			</p:complexElementInstance>
			<!-- <p:complexElementInstance groupID="orientation_field" XPath="swe:field"> -->
			<!-- <p:elements> -->
			<!-- <p:elementInstanceNCName ID="name_orientation_field" XPath="@name"> -->
			<!-- <p:restrictions> -->
			<!-- <p:fixContent> -->
			<!-- <p:value>orientation</p:value> -->
			<!-- </p:fixContent> -->
			<!-- </p:restrictions> -->
			<!-- </p:elementInstanceNCName> -->
			<!-- </p:elements> -->
			<!-- </p:complexElementInstance> -->
		</p:elements>
	</p:complexElementInstance>

	<p:complexElementInstance groupID="longitude_Quantity" XPath="swe:coordinate/swe:Quantity">
		<p:elements>
			<p:elementInstanceAnyURI ID="referenceFrame_longitude_Quantity" XPath="@referenceFrame">
				<p:restrictions>
					<p:fixContent>
						<p:value>urn:ogc:crs:EPSG:4326</p:value>
					</p:fixContent>
				</p:restrictions>
			</p:elementInstanceAnyURI>
			<p:elementInstanceString ID="axisID_longitude_Quantity" XPath="@axisID">
				<p:restrictions>
					<p:fixContent>
						<p:value>Lon</p:value>
					</p:fixContent>
				</p:restrictions>
			</p:elementInstanceString>
			<p:elementInstanceString ID="code_uom_longitude_Quantity" XPath="swe:uom/@code">
				<p:restrictions>
					<p:defaultContent>
						<p:value>deg</p:value>
					</p:defaultContent>
				</p:restrictions>
			</p:elementInstanceString>
			<p:elementInstanceDouble ID="value_longitude_Quantity" XPath="swe:value">
				<p:restrictions>
					<p:defaultContent>
						<p:value>0</p:value>
					</p:defaultContent>
				</p:restrictions>
			</p:elementInstanceDouble>
		</p:elements>
	</p:complexElementInstance>

	<p:complexElementInstance groupID="latitude_Quantity" XPath="swe:coordinate/swe:Quantity">
		<p:elements>
			<p:elementInstanceAnyURI ID="referenceFrame_latitude_Quantity" XPath="@referenceFrame">
				<p:restrictions>
					<p:fixContent>
						<p:value>urn:ogc:crs:EPSG:4326</p:value>
					</p:fixContent>
				</p:restrictions>
			</p:elementInstanceAnyURI>
			<p:elementInstanceString ID="axisID_latitude_Quantity" XPath="@axisID">
				<p:restrictions>
					<p:fixContent>
						<p:value>Lat</p:value>
					</p:fixContent>
				</p:restrictions>
			</p:elementInstanceString>
			<p:complexElementInstance groupID="uom_latitude_Quantity" XPath="swe:uom">
				<p:elements>
					<p:elementInstanceString ID="code_uom_latitude_Quantity" XPath="@code">
						<p:restrictions>
							<p:defaultContent>
								<p:value>deg</p:value>
							</p:defaultContent>
						</p:restrictions>
					</p:elementInstanceString>
				</p:elements>
			</p:complexElementInstance>
			<p:elementInstanceDouble ID="value_latitude_Quantity" XPath="swe:value">
				<p:restrictions>
					<p:defaultContent>
						<p:value>0</p:value>
					</p:defaultContent>
				</p:restrictions>
			</p:elementInstanceDouble>
		</p:elements>
	</p:complexElementInstance>

	<p:complexElementInstance groupID="altitude_Quantity" XPath="swe:coordinate/swe:Quantity">
		<p:elements>
			<p:elementInstanceAnyURI ID="referenceFrame_altitude_Quantity" XPath="@referenceFrame">
				<p:restrictions>
					<p:fixContent>
						<p:value>urn:ogc:crs:EPSG:5715</p:value>
					</p:fixContent>
				</p:restrictions>
			</p:elementInstanceAnyURI>
			<p:elementInstanceString ID="axisID_altitude_Quantity" XPath="@axisID">
				<p:restrictions>
					<p:fixContent>
						<p:value>Alt</p:value>
					</p:fixContent>
				</p:restrictions>
			</p:elementInstanceString>
			<p:complexElementInstance groupID="uom_altitude_Position" XPath="swe:uom">
				<p:elements>
					<p:elementInstanceString ID="code_uom_altitude_Position" XPath="@code">
						<p:restrictions>
							<p:defaultContent>
								<p:value>m</p:value>
							</p:defaultContent>
						</p:restrictions>
					</p:elementInstanceString>
				</p:elements>
			</p:complexElementInstance>
			<p:elementInstanceDouble ID="value_altitude_Quantity" XPath="swe:value">
				<p:restrictions>
					<p:defaultContent>
						<p:value>0</p:value>
					</p:defaultContent>
				</p:restrictions>
			</p:elementInstanceDouble>
		</p:elements>
	</p:complexElementInstance>

</p:profile>
